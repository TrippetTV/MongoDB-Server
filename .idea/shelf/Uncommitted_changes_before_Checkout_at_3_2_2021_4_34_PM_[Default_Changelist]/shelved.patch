<+><html lang=\"en\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\"\r\n          content=\"width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0\">\r\n\r\n    <link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css\" rel=\"stylesheet\" integrity=\"sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1\" crossorigin=\"anonymous\">\r\n    <link rel=\"stylesheet\" href=\"css/dimmer.css\">\r\n    <link rel=\"stylesheet\" href=\"css/home.css\">\r\n\r\n    <title>Document</title>\r\n</head>\r\n<body>\r\n    <div class=\".container-fluid\">\r\n        <div class=\"row align-items-center\">\r\n            <nav class=\"navbar navbar-expand-lg navbar-dark bg-dark\">\r\n                <div class=\"col\">\r\n                    <a class=\"navbar-brand\" href=\"/\">Bookface</a>\r\n                </div>\r\n                <div class=\"col\">\r\n                    <div style=\"text-align: center\">\r\n                        <!-- TODO Make button toggleable or add cancel button -->\r\n                        <button class=\"btn btn-outline-info\" id=\"post-create\">Create Post</button>\r\n                    </div>\r\n                </div>\r\n                <div class=\"col\">\r\n                    <div class=\"d-flex justify-content-end btn-toolbar\">\r\n                            <div class=\"btn-group mr-2\">\r\n                                <a href=\"/create\"><button class=\"btn btn-outline-info\">Signup</button></a>\r\n                                <a href=\"/loginpage\"><button class=\"btn btn-outline-info\">Login</button></a>\r\n                            </div>\r\n                        </div>\r\n                </div>\r\n            </nav>\r\n        </div>\r\n    </div>\r\n    <div class=\"row\">\r\n        <div class=\"col\">\r\n\r\n        </div>\r\n        <div class=\"col\" id=\"post-creation\">\r\n            <form action=\"/\" method=\"post\" id=\"postmessage\">\r\n                <label for=\"message\"></label>\r\n                <textarea name=\"message\" id=\"message\" form=\"postmessage\" placeholder=\"What's on your mind?\"></textarea>\r\n                <input type=\"submit\" class=\"btn btn-outline-success\" value=\"Post\">\r\n            </form>\r\n            <div class=\"dimmer\">\r\n                <span class=\"exit\">&times;</span>\r\n            </div>\r\n        </div>\r\n        <div class=\"col\">\r\n\r\n        </div>\r\n    </div>\r\n    <div class=\"row\">\r\n        <div class=\"col\">\r\n\r\n        </div>\r\n        <div id=\"posts\" class=\"col\">\r\n            <% for (var i = Object.keys(data).length; i > 0; i--) { %>\r\n            <div class=\"card\" style=\"width: 100%\">\r\n                <div class=\"card-body\">\r\n                    <h5 class=\"card-title\">Posted by <%= i.User %> </h5>\r\n                    <h6 class=\"card-subtitle mb-2 text-muted\">\r\n                        <% let dd = String(i.date.getDate()).padStart(2, '0'); %>\r\n                        <% let mm = String(i.date.getMonth() + 1).padStart(2, '0'); %>\r\n                        <% let yyyy = i.date.getFullYear(); %>\r\n\r\n                        <%= dd + '/' + mm + '/' + yyyy %>\r\n                    </h6>\r\n                    <p class=\"card-text\"> <%= i.message %> </p>\r\n                    <a href=\"\" class=\"card-link\">Edit Message</a>\r\n                    <a href=\"\" class=\"card-link\">Delete Message</a>\r\n                </div>\r\n            </div>\r\n            <% } %>\r\n        </div>\r\n        <div class=\"col\">\r\n\r\n        </div>\r\n    </div>\r\n\r\n\r\n</body>\r\n</html>\r\n<script>\r\n    let check = false;\r\n    const button = document.querySelector(\"#post-create\")\r\n    const posts = document.querySelector(\"#post-creation\")\r\n    const txt = document.querySelector(\"#message\")\r\n    button.addEventListener(\"click\", (e) => {\r\n        //const dimmer = $('.dimmer');\r\n        if (!check) {\r\n            posts.style.visibility = \"visible\"\r\n            txt.style.height = \"250px\"\r\n            check = true\r\n        }\r\n    })\r\n        exit = document.getElementsByClassName(\"exit\")\r\n        exit.addEventListener(\"onclick\", (e) => {\r\n            check = false\r\n            txt.style.height = \"0\"\r\n            posts.style.visibility = \"hidden\"\r\n\r\n        })\r\n</script>
